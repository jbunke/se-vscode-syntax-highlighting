{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Stipple Effect extension",
	"patterns": [
        { "include": "source.ds" }, 
		{ "include": "#ext_types" }, 
        { "include": "#namespace" }
	],
	"repository": {
		"ext_types": {
			"name": "entity.name.type",
			"match": "\\b(layer|palette|project|save_config|script)\\b"
		}, 
        "namespace": {
            "match": "\\$(SE|Graphics|Math)\\b", 
            "name": "emphasis"
        }
	},
	"scopeName": "source.ses"
}
